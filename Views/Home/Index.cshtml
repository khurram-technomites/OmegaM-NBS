												<div class="col-md-6 col-sm-12">
													<div class="form-group w-100">
														<div class="wo-form-icon">
															<input type="text" class="time form-control" id="EndTime" name="EndTime" required placeholder="@Resources.Time">
															<a href="javascript:void(0);" class="wo-right-icon input-group-addon">
																<i class="ti-time"></i>
															</a>
														</div>
													</div>
												</div>
											</div>
										</div>
									</div>
									<div class="row">
										<div class="col-12">
											<button type="submit" class="btn btn-block btn-theme btn-search-vehicle">
												@Resources.SearchVehicle
											</button>
										</div>
									</div>
								</fieldset>
							</form>
						</div>
					</div>
				</div>
			</div>
