<!-- Map Modal Start -->
<div class="modal fade wo-videopopup" tabindex="-1" role="dialog" id="map-modal">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="wo-modalcontent modal-content">
			<div class="modal-body">
				<a href="javascript:void(0);" class="close"><i class="ti-close" data-dismiss="modal"></i></a>
				<div class="login-header">
					<h4 class="text-center mb-2">@Resources.SearchLocation</h4>
					<p class="text-center" id="drag-map"><span>@Resources.DragMarker</span></p>
				</div>
				<!-- Modal Body Start -->
				<div class="container">
					<div class="row">
						<div class="col-lg-12">
							<div class="map-div" style="display:none">
								<input type="text" class="form-control mb-3 txtAddress" id="current-location" name="current-location" placeholder="@Resources.SearchLocation" required="">
								<div id="googleMap" style="width:100%;height:400px;"></div>
								<button class="btn btn-block btn-theme border-radius-4 mt-3 p-3" type="button" data-dismiss="modal">@Resources.Done</button>
							</div>
							<div class="map-div-spin p-5 m-5 text-center">
								<span class="fa fa-circle-notch fa-spin fa-2x"></span>
							</div>
						</div>
					</div>
				</div>
				<!-- Modal Body End -->
			</div>
		</div>
	</div>
</div>
<!-- Map Modal End -->
