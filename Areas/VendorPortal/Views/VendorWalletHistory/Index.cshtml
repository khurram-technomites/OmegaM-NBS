    @if (ViewBag.ExcelUploadErrorMessage != null)
    {
        <script>
			swal.fire({
				title: '@ViewBag.ErrorMessage',
				html: '@Html.Raw(ViewBag.ExcelUploadErrorMessage)',
				type: 'error',
			});
        </script>
    }






    <script src="//cdnjs.cloudflare.com/ajax/libs/numeral.js/2.0.6/numeral.min.js"></script>
    <script>
		$(document).ready(function () {

            $("#earning").text(numeral(@Model.TotalEarning).format('0,0.00'));
            $("#pending").text(numeral(@Model.PendingAmount).format('0,0.00'));
            $("#transfered").text(numeral(@Model.TransferedAmount).format('0,0.00'));
		});

    </script>
