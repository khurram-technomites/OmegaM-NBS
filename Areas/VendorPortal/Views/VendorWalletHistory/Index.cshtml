                                <i class="fa fa-shopping-cart fa-2x stats-color"></i>
                            </span>
                            <div class="text-dark font-weight-bolder font-size-h2 mt-3" id="transfered"></div>
                            <a href="javascript:void(0);" class="text-muted text-hover-primary font-weight-bold font-size-lg mt-1">Transfered</a>
                        </div>
                    </div>
                    <!--end::Tiles Widget 12-->
                </div>
            </div>
