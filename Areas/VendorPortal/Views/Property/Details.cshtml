<script>

    $(document).ready(function () {

        //$("#BuildYear").datepicker({ dateFormat: 'yy' });
        //$('#BuildYear').datepicker({
        //    format: "yyyy",
        //    viewMode: "years",
        //    minViewMode: "years",
        //    autoclose: true //to close picker once year is selected
        //});

        if ('@Model.CategoryId' != null) {
            $("#CategoryId").val('@Model.CategoryId')
        }

        if ('@Model.ForSale' != null) {
            $("#ForSale").val('@Model.ForSale.ToString()')
        }

        if ('@Model.CountryID' != null) {
            $("#CountryId").val('@Model.CountryID.ToString()')
        }

        if ('@Model.CityID' != null) {
            $.ajax({
                type: "GET",
                url: "/Vendor/Property/GetCities?CountryId=" + $("#CountryId").val(),
                data: "{}",
                success: function (data) {
                    var s = '<option value="-1">Select City</option>';
                    for (var i = 0; i < data.length; i++) {
                        if (data[i].value == '@Model.CityID.ToString()') {
                            s += '<option value="' + data[i].value + '" selected>' + data[i].text + '</option>';
                        } else {
                            s += '<option value="' + data[i].value + '">' + data[i].text + '</option>';
                        }
                    }
                    $("#City").html(s);
                }
            });
            $("#City").prop("disabled", true);
        }


        $('#IsFurnished').val(@Model.IsFurnished);


    })

</script>

