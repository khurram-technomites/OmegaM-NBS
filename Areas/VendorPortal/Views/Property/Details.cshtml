                            <!--begin::Accordion-->
                            <div class="accordion accordion-light accordion-toggle-plus accordion-light-borderless accordion-svg-toggle" id="accordionFeatures">
                                <div class="card">
                                    <div class="card-header" id="headingCategories">
                                        <div class="card-title" data-toggle="collapse" data-target="#collapseFeatures">
                                            <div class="card-label pl-4">Features</div>
                                        </div>
                                    </div>
                                    <div id="collapseFeatures" class="collapse show" data-parent="#accordionFeatures">
                                        <div class="card-body pl-12">
                                            <div class="form-group">
                                                @*<input type="search" class="form-control" onkeyup="SearchFeatures(this)" onchange="SearchFeatures(this)" placeholder="Search Features">*@
                                                <div class="checkbox-list prop-features mt-2">

                                                    @{
                                                        foreach (var item in ViewBag.Features)
                                                        {
                                                            if (Model.PropertyFeatures.Any(x => x.FeatureId == item.ID))
                                                            {
                                                                <div class="car-category car-category-plain mb-1" id="Features" parent-id="null">
                                                                    <label class="checkbox">
                                                                        <input type="checkbox" value="@item.ID" checked disabled>
                                                                        <span></span>@item.Name
                                                                    </label>
                                                                </div>
                                                            }
                                                            else
                                                            {
                                                                <div class="car-category car-category-plain mb-1" id="Features" parent-id="null">
                                                                    <label class="checkbox">
                                                                        <input type="checkbox" value="@item.ID" disabled>
                                                                        <span></span>@item.Name
                                                                    </label>
                                                                </div>
                                                            }
                                                        }
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--end::Accordion-->
                        </div>
                    </div>

