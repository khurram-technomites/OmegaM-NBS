<style>
    .card-custom .card-body {
        padding: 10px 20px !important;
    }

    .symbol {
        box-shadow: 2px 2px 5px #0000004d;
    }

    .btn-remove-gallery-image {
        position: absolute;
        right: -10px;
        top: -10px;
    }

    .symbol .spinner.spinner-right:before {
        right: 0rem;
    }

    .symbol .spinner.spinner-right.btn:not(.btn-block) {
        padding-right: unset;
    }

    .wizard.simple-car .variat-car, .wizard.variat-car .simple-car {
        display: none;
    }
