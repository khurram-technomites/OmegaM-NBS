            var fuelType = '@Model.FuelType';
            if(fuelType)
            {
                $('#FuelType').closest('.form-group').find(`span:contains(${fuelType})`).addClass('active');
                $('#FuelType').val(fuelType);
            }

            var transmission = '@Model.Transmission';
            if(transmission)
            {
                $('#Transmission').closest('.form-group').find(`span:contains(${transmission})`).addClass('active');
                $('#Transmission').val(transmission);
            }
            var doors = '@Model.Doors';
            if(doors)
            {
