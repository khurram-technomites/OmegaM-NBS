                // minimum setup
                $('.kt-selectpicker').selectpicker();
            }

            return {
                // public functions
                init: function () {
                    demos();
                }
            };
        }();

        // Initialization
        jQuery(document).ready(function () {
            KTBootstrapSelect.init();
            //KTCkeditor.init();

            if ($('#CarMakeID').val() == "") {

                $('#CarModelID').empty();
                $("#CarModelID").prepend("<option value=''  selected>Select car make first...</option>");
            }
            else {
                //$("#CarMakeID").trigger("change");
                $("#CarModelID").val('@Model.CarModelID');
                @*$("#CarModelID").prepend("<option value='@Model.CarModelID' selected></option>");*@
            }

            if ($('#CarModelID').val() == "") {
                $('#CarModelID').empty();
                $("#CarModelID").prepend("<option value=''  selected>Select car make first...</option>");
            }

            $('#CarType').val('@Model.Type');
            $("#CategoryID").val('@Model.CategoryID');
            $("#CarMakeID").val('@Model.CarMakeID');
            $("#BodyTypeID").val('@Model.BodyTypeID');

