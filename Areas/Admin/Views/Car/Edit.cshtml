    <script>
        var slideIndex = 1;
        showDivs(slideIndex);

        function plusDivs(n) {
            slideIndex = parseInt($("#imagepreview").attr("data-id"));
            showDivs(slideIndex += n);
        }

        function showDivs(n) {
            var i;
            var x = document.getElementsByClassName("car-image");

            if (n >= x.length) { slideIndex = 0 }
            if (n < 0) { slideIndex = x.length-1 }
            for (i = 0; i < x.length; i++) {
                $('#imagepreview').attr('src', x[slideIndex].style.backgroundImage.slice(5, -2))
                $('#imagepreview').attr('data-id', x[slideIndex].dataset.id)
            }
           /* x[slideIndex - 1].style.display = "block";*/
        }
        var Description, DescriptionAr;

        'use strict';
        $('#CarMakeID').attr("data-live-search", "true");
        $('#BodyTypeID').attr("data-live-search", "true");
        $('#CategoryID').attr("data-live-search", "true");
        //$('#CarModelID').attr("data-live-search", "true");
        var arrows;
        if (KTUtil.isRTL()) {
            arrows = {
                leftArrow: '<i class="la la-angle-right"></i>',
                rightArrow: '<i class="la la-angle-left"></i>'
            }
        } else {
            arrows = {
                leftArrow: '<i class="la la-angle-left"></i>',
                rightArrow: '<i class="la la-angle-right"></i>'
            }
        }


        //// Date Range
        //$('.kt_datepicker_range').datepicker({
        //	rtl: KTUtil.isRTL(),
