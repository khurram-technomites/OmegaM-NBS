@model NowBuySell.Data.Car
@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<link href="~/Assets/Admin/css/vendor/car/edit.css" rel="stylesheet" />
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

<style>
    body.modal-open {
        height: 100vh;
        overflow: visible !important;
        padding-right: 15px;
    }
</style>
@Html.AntiForgeryToken()
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-12 subheader-transparent" id="kt_subheader">
        <div class="container-fluid align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Heading-->
                <div class="d-flex flex-column">

                    <!--begin::Breadcrumb-->
                    <div class="d-flex align-items-center font-weight-bold my-2">
                        <!--begin::Item-->
                        <a href="/Admin/Dashboard/Index" class="opacity-75 hover-opacity-100">
                            <i class="flaticon2-shelter text-white icon-1x"></i>
                        </a>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <a href="/Admin/Dashboard/Index" class="text-white text-hover-white opacity-75 hover-opacity-100">&nbsp;&nbsp;Dashboard</a>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <span class="label label-dot label-sm bg-white opacity-75 mx-3"></span>
                        <a href="javascript: window.history.go(-1);" class="text-white text-hover-white opacity-75 hover-opacity-100">Motors</a>
                        <span class="label label-dot label-sm bg-white opacity-75 mx-3"></span>
                        <a href="javascript:;" class="text-white text-hover-white opacity-75 hover-opacity-100">Edit</a>
                        <!--end::Item-->
                    </div>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Heading-->
            </div>
            <!--end::Info-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <!--begin::Dashboard-->
            <div class="row">
                <div class="col-md-12">
                    <div class="card card-custom  gutter-b">
                        <div class="card-body">
                            <!--begin::Accordion-->
                            <div class="accordion accordion-light accordion-toggle-plus accordion-light-borderless accordion-svg-toggle" id="accordionBasicDetails">
                                <div class="card">
                                    <div class="card-header" id="headingBasicDetails">
                                        <div class="card-title" data-toggle="collapse" data-target="#collapseBasicDetails">
                                            <div class="card-label pl-4">About Motor  </div>
                                        </div>
                                    </div>
                                    <div id="collapseBasicDetails" class="collapse show" data-parent="#accordionBasicDetails">
                                        <div class="card-body pl-12">
                                            <form id="btnFormSubmit">
                                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                                @Html.HiddenFor(model => model.ID)
                                                <div class="row">
                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Ads Reference Code</label>
                                                            <div class="input-group mb-2">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text">
                                                                        <i class="fa fa-hashtag"></i>
                                                                    </span>
                                                                </div>
                                                                <input type="text" class="form-control" required readonly name="AdsReferenceCode" id="AdsReferenceCode" value="@Model.AdsReferenceCode" />
                                                                @Html.ValidationMessageFor(model => model.AdsReferenceCode, "", new { @class = "text-danger" })

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Slug</label>
                                                            <div class="input-group mb-2">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text">
                                                                        <i class="fa fa-link"></i>
                                                                    </span>
                                                                </div>
                                                                <input type="text" class="form-control" required readonly name="Slug" id="Slug" placeholder="Enter car name" value="@Model.Slug" />
                                                                @Html.ValidationMessageFor(model => model.Slug, "", new { @class = "text-danger" })

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <label class="control-label">
                                                            Title
                                                        </label>
                                                        <div class="form-group">
                                                            <div class="input-group mb-2">
                                                                @Html.EditorFor(model => model.Name, new { htmlAttributes = new { @class = "form-control", placeholder = "Car Name", required = "required" } })
                                                                @Html.ValidationMessageFor(model => model.Name, "", new { @class = "text-danger" })
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text"><i class="fa fa-tag"></i></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <label class="control-label text-right">
                                                            Title (Ar)
                                                        </label>
                                                        <div class="form-group">
                                                            <div class="input-group mb-2">
                                                                @Html.EditorFor(model => model.NameAr, new { htmlAttributes = new { @class = "form-control", placeholder = "اسم المنتج", required = "required", dir = "rtl" } })
                                                                @Html.ValidationMessageFor(model => model.NameAr, "", new { @class = "text-danger" })
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text"><i class="fa fa-tag"></i></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*<input type="text" class="" value="-" name="NameAr" id="NameAr"/>*@
                                                </div>

                                                <div class="row">


                                                </div>


                                                <div class="row">


                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <row>
                                                                <label class="control-label">Motor Make</label>
                                                                @*<a href="javascript:;" onclick="OpenModelPopup(this,'/Vendor/Car/AddCarMake')">Request New</a>*@
                                                            </row>

                                                            <div class="input-group mb-3">
                                                                @Html.DropDownList("CarMakeID", null, "Select car make", htmlAttributes: new { @class = "form-control kt-selectpicker", @data_actions_box = "true", id = "CarMakeID", required = "required" })
                                                                @Html.ValidationMessageFor(model => model.CarMakeID, "", new { @class = "text-danger" })
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text"><i class="fa fa-tools"></i></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Motor Model </label>
                                                            @*<a href="javascript:;" onclick="OpenModelPopup(this,'/Vendor/Car/AddCarModel')">Request New</a>*@
                                                            @*<div class="input-group mb-3">

                                                                    <select class="form-control select" name="CarModelID" id="CarModelID">
                                                                        <option value="">None</option>

                                                                    </select>
                                                                    <div class="input-group-append">
                                                                        <span class="input-group-text"><i class="fa fa-car"></i></span>
                                                                    </div>
                                                                </div>*@
                                                            <div class="input-group mb-3">
                                                                @Html.DropDownList("CarModelID", null, "Select motor model type", htmlAttributes: new { @class = "form-control", id = "CarModelID", required = "required" })
                                                                @Html.ValidationMessageFor(model => model.CarModelID, "", new { @class = "text-danger" })
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text"><i class="fa fa-car"></i></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Category</label>
                                                            <div class=" row"></div>
                                                            <div class="input-group mb-3">
                                                                @Html.DropDownList("CategoryID", null, "Select car category", htmlAttributes: new { @class = "form-control kt-selectpicker", @data_actions_box = "true", id = "CategoryID", required = "required" })
                                                                @Html.ValidationMessageFor(model => model.CategoryID, "", new { @class = "text-danger" })
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text"><i class="fa fa-car"></i></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Body Type</label>
                                                            @*<a href="javascript:;" onclick="OpenModelPopup(this,'/Vendor/Car/AddBodyType')">Request New</a>*@
                                                            <div class=" row"></div>
                                                            <div class="input-group mb-3">
                                                                @Html.DropDownList("BodyTypeID", null, "Select body type", htmlAttributes: new { @class = "form-control kt-selectpicker", @data_actions_box = "true", id = "BodyTypeID", required = "required" })
                                                                @Html.ValidationMessageFor(model => model.BodyTypeID, "", new { @class = "text-danger" })
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text"><i class="fa fa-car"></i></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-3 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">VIN / Chassis Number (Optional)</label>
                                                            <div class="input-group mb-2">
                                                                <input type="text" class="form-control" name="SKU" id="SKU" placeholder="Enter car name" value="@Model.SKU" />
                                                                @Html.ValidationMessageFor(model => model.SKU, "", new { @class = "text-danger" })
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text">
                                                                        <i class="fa fa-link"></i>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Year</label>
                                                            <div class="input-group mb-2">
                                                                <input type="text" class="form-control  carYear" required name="Year" id="datepicker" placeholder="Enter year" value="@Model.Year" />
                                                                @Html.ValidationMessageFor(model => model.Year, "", new { @class = "text-danger" })
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text">
                                                                        <i class="fa fa-calendar"></i>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Kilometers Driven</label>
                                                            <div class="input-group mb-2">
                                                                <input type="number" min="0" class="form-control" required name="FuelEconomy" id="FuelEconomy" placeholder="Enter mileage / fuel economy  " value="@Model.FuelEconomy" />
                                                                @Html.ValidationMessageFor(model => model.FuelEconomy, "", new { @class = "text-danger" })
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text">
                                                                        <i class="fa fa-gas-pump"></i>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-3 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Price</label>
                                                            <div class="input-group mb-2">
                                                                <input type="number" class="form-control" min="0" required name="SalePrice" id="SalePrice" placeholder="Enter price" value="@Model.SalePrice" />
                                                                @Html.ValidationMessageFor(model => model.SalePrice, "", new { @class = "text-danger" })
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text">
                                                                        <i class="fa fa-money-bill"></i>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>


                                                </div>
                                                <div class="row">
                                                    <div class="col-md-6 col-sm-12">

                                                        <div class="form-group">
                                                            <label class="control-label">Fuel Type</label>
                                                            <div class="input-group mb-2">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text"><i class="fa fa-gas-pump"></i></span>
                                                                </div>
                                                                <div class="tab-options">
                                                                    <span>Petrol</span>
                                                                    <span>Diesel</span>
                                                                    <span>Hybrid</span>
                                                                    <span>Electric</span>
                                                                </div>
                                                                <select id="FuelType" name="FuelType" required>
                                                                    <option value="">Select Fuel Type ...</option>
                                                                    <option value="Petrol">Petrol</option>
                                                                    <option value="Diesel">Diesel</option>
                                                                    <option value="Hybrid">Hybrid</option>
                                                                    <option value="Electric">Electric</option>
                                                                </select>

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Transmission</label>

                                                            <div class="input-group mb-3">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text"><i class="fa fa-wrench"></i></span>
                                                                </div>
                                                                <div class="tab-options">
                                                                    <span title="Automatic transmission">Automatic</span>
                                                                    <span title="Manual transmission">Manual</span>
                                                                    <span title="Continuously variable transmission (CVT)">CVT</span>
                                                                    <span title="Semi-automatic and dual-clutch transmissions">DCT</span>
                                                                </div>
                                                                <select id="Transmission" name="Transmission" required>
                                                                    <option value="" selected disabled>Select Transmission ...</option>
                                                                    <option value="Manual">Manual transmission.</option>
                                                                    <option value="Automatic">Automatic transmission.</option>
                                                                    <option value="CVT">Continuously variable transmission (CVT)</option>
                                                                    <option value="DCT">Semi-automatic and dual-clutch transmissions.</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Number of Doors</label>
                                                            <div class="input-group mb-3">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text"><i class="fa fa-car"></i></span>
                                                                </div>
                                                                <div class="tab-options">
                                                                    <span>2</span>
                                                                    <span>3</span>
                                                                    <span>4</span>
                                                                    <span>5+</span>
                                                                </div>
                                                                <select id="Doors" name="Doors" required style="opacity:0">
                                                                    <option value="">Select doors ...</option>
                                                                    <option value="2">2 Doors</option>
                                                                    <option value="3">3 Doors</option>
                                                                    <option value="4">4 Doors</option>
                                                                    <option value="5+">5+ Doors</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Number of Wheels</label>
                                                            <div class="input-group mb-3">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text"><i class="fa fa-ring"></i></span>
                                                                </div>
                                                                <div class="tab-options">
                                                                    <span>2</span>
                                                                    <span>3</span>
                                                                    <span>4</span>
                                                                    <span>6+</span>
                                                                </div>
                                                                <select id="Wheels" name="Wheels" required>
                                                                    <option value="">Select wheels ...</option>
                                                                    <option value="2">2 Wheels</option>
                                                                    <option value="3">3 Wheels</option>
                                                                    <option value="4">4 Wheels</option>
                                                                    <option value="6+">6+ Wheels</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>



                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <row>
                                                                <label class="control-label">Capacity</label>
                                                            </row>

                                                            <div class="input-group mb-3">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text"><i class="fa fa-users"></i></span>
                                                                </div>
                                                                <div class="tab-options">
                                                                    <span>1 - 2 Persons</span>
                                                                    <span>2 - 4 Persons</span>
                                                                    <span>4 - 6 Persons</span>
                                                                    <span>6 - 10 Persons</span>
                                                                </div>
                                                                <select id="Capacity" name="Capacity" style="opacity:0">
                                                                    <option value="">Select capacity ...</option>
                                                                    <option value="1 - 2 Persons">1 - 2 Persons</option>
                                                                    <option value="2 - 4 Persons">2 - 4 Persons</option>
                                                                    <option value="4 - 6 Persons">4 - 6 Persons</option>
                                                                    <option value="6 - 10 Persons">6 - 10 Persons</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Steering Side</label>
                                                            <div class="input-group mb-2">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text"><i class="fa fa-car"></i></span>
                                                                </div>
                                                                <div class="tab-options">
                                                                    <span>Left-Hand Side</span>
                                                                    <span>Right-Hand Side</span>
                                                                </div>
                                                                <select id="SteeringSide" name="SteeringSide" required>
                                                                    <option value="">Select Steering Side ...</option>
                                                                    <option value="Left-Hand Side">Left-Hand Side</option>
                                                                    <option value="Right-Hand Side">Right-Hand Side</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Body Condition</label>
                                                            <div class="input-group mb-2">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text"><i class="fa fa-tag"></i></span>
                                                                </div>
                                                                <div class="tab-options">
                                                                    <span>1 - 3</span>
                                                                    <span>4 - 6</span>
                                                                    <span>7 - 9</span>
                                                                    <span>10 - 10</span>
                                                                </div>

                                                                <select id="Condition" name="Condition" required>
                                                                    <option value="">Select Condition ...</option>
                                                                    <option value="1 - 3">1 - 3</option>
                                                                    <option value="4 - 6">4 - 6</option>
                                                                    <option value="7 - 9">7 - 9</option>
                                                                    <option value="10 - 10">10 - 10</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-6 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Mechincal Condition</label>
                                                            <div class="input-group mb-2">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text"><i class="fa fa-tag"></i></span>
                                                                </div>
                                                                <div class="tab-options">
                                                                    <span>Perfect inside</span>
                                                                    <span>Perfect out</span>
                                                                    <span>Perfect inside & out</span>
                                                                </div>
                                                                <select id="MechanicalCondition" name="MechanicalCondition" required>
                                                                    <option value="">Select Mechanical Condition ...</option>
                                                                    <option value="Perfect inside">Perfect inside</option>
                                                                    <option value="Perfect out">Perfect out</option>
                                                                    <option value="Perfect inside & out">Perfect inside & out</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-8 col-sm-12">
                                                        <div class="form-group">
                                                            <row>
                                                                <label class="control-label">Cylinders</label>
                                                            </row>

                                                            <div class="input-group mb-3">
                                                                <div class="input-group-prepend" readonly>
                                                                    <span class="input-group-text"><i class="fa fa-tools"></i></span>
                                                                </div>
                                                                <div class="tab-options">
                                                                    <span>3</span>
                                                                    <span>4</span>
                                                                    <span>5</span>
                                                                    <span>6</span>
                                                                    <span>8</span>
                                                                    <span>10</span>
                                                                    <span>12</span>
                                                                    <span>16</span>
                                                                    <span>N/A, Electric</span>
                                                                    <span>Not Sure</span>
                                                                </div>
                                                                <select id="Cylinders" name="Cylinders" required>
                                                                    <option value="">Select cylinders ...</option>
                                                                    <option value="3">3</option>
                                                                    <option value="4">4</option>
                                                                    <option value="5">5</option>
                                                                    <option value="6">6</option>
                                                                    <option value="8">8</option>
                                                                    <option value="10">10</option>
                                                                    <option value="12">12</option>
                                                                    <option value="N/A, Electric">N/A, Electric</option>
                                                                    <option value="Not Sure">Not Sure</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Warranty</label>
                                                            <span class="switch">
                                                                <label>
                                                                    @Html.CheckBox("Warranty")
                                                                    <span></span>
                                                                </label>
                                                            </span>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-2 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Service History</label>
                                                            <span class="switch">
                                                                <label>
                                                                    @Html.CheckBox("ServiceHistory")
                                                                    <span></span>
                                                                </label>
                                                            </span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">

                                                    <div class="col-md-4 col-sm-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Horse Power</label>
                                                            <div class="input-group mb-3">
                                                                <select id="HorsePower" name="HorsePower" required class="form-control">
                                                                    <option value="">Select horse power ...</option>
                                                                    <option value="Less Than 150 HP">Less Than 150 HP</option>
                                                                    <option value="150 - 200 HP">150 - 200 HP</option>
                                                                    <option value="200 - 300 HP">200 - 300 HP</option>
                                                                    <option value="300 - 400 HP">300 - 400 HP</option>
                                                                    <option value="400 - 500 HP">400 - 500 HP</option>
                                                                    <option value="500 - 600 HP">500 - 600 HP</option>
                                                                    <option value="600 - 700 HP">600 - 700 HP</option>
                                                                    <option value="700 - 800 HP">700 - 800 HP</option>
                                                                    <option value="800 - 900 HP">800 - 900 HP</option>
                                                                    <option value="900+ HP">900+ HP</option>
                                                                    <option value="Not Sure">Not Sure</option>
                                                                    <option value="Other">Other</option>
                                                                </select>
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text"><i class="fa fa-car"></i></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-4 col-sm-12">
                                                        <div class="form-group">
                                                            <row>
                                                                <label class="control-label">Regional Specs</label>
                                                            </row>

