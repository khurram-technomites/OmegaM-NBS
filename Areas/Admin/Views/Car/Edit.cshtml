                $('#SteeringSide').val(steeringSide);
            }

            var condition = '@Model.Condition';
            if(condition)
            {
                $('#Condition').closest('.form-group').find(`span:contains(${condition})`).addClass('active');
                $('#Condition').val(condition);
            }

            var mechanicalCondition = '@Model.MechanicalCondition';
            if(mechanicalCondition)
            {
                $('#MechanicalCondition').closest('.form-group').find(`span:contains(${mechanicalCondition.replace("&amp;", "&")})`).addClass('active');
                $('#MechanicalCondition').val(mechanicalCondition.replace("&amp;", "&"));
            }

            var cylinders = '@Model.Cylinders';
            if(cylinders)
            {
                $('#Cylinders').closest('.form-group').find(`span:contains(${cylinders})`).addClass('active');
                $('#Cylinders').val(cylinders);
            }
        });


        $('#kt_datetimepicker_1').datetimepicker();


