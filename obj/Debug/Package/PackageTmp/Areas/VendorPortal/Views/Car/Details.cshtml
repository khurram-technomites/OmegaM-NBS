
@model NowBuySell.Data.Car
@{
    ViewBag.Title = "Motor Details";
    Layout = "~/Areas/VendorPortal/Views/Shared/_Layout.cshtml";
}

<link href="~/Assets/Admin/css/vendor/car/edit.css" rel="stylesheet" />
<!--begin::Page Custom Styles(used by this page)-->
<link href="~/assets/admin/css/pages/wizard/wizard-2.css" rel="stylesheet" type="text/css" />
<link href="~/assets/Admin/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" />
<!--end::Page Custom Styles-->
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-12 subheader-transparent" id="kt_subheader">
        <div class="container-fluid align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Heading-->
                <div class="d-flex flex-column">

                    <!--begin::Breadcrumb-->
                    <div class="d-flex align-items-center font-weight-bold my-2">
                        <!--begin::Item-->
                        <a href="/Admin/Dashboard/Index" class="opacity-75 hover-opacity-100">
                            <i class="flaticon2-shelter text-white icon-1x"></i>
                        </a>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <a href="/Admin/Dashboard/Index" class="text-white text-hover-white opacity-75 hover-opacity-100">&nbsp;&nbsp;Dashboard</a>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <span class="label label-dot label-sm bg-white opacity-75 mx-3"></span>
                        <a href="javascript: window.history.go(-1);" class="text-white text-hover-white opacity-75 hover-opacity-100">Cars</a>
                        <span class="label label-dot label-sm bg-white opacity-75 mx-3"></span>
                        <a href="javascript:;" class="text-white text-hover-white opacity-75 hover-opacity-100">Details</a>
                        <!--end::Item-->
                    </div>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Heading-->
            </div>
            <!--end::Info-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container-fluid">
            <!--begin::Dashboard-->
            <div class="row">
                <div class="col-md-9">
                    <!--begin::Card-->
                    @*<div class="card card-custom  gutter-b" id="kt_page_sticky_card">
                            <div class="card-body">

                            </div>
                        </div>*@
                    <div class="card card-custom  gutter-b">
                        <div class="card-body">
                            <h3 class="card-title">
                                Motor Details
                            </h3>
                            <div class="row">
                                <div class="col-md-6 ">
                                    <div class="form-group">
                                        <label>Motor Title</label>
                                        <div class="input-group mb-2">
                                            @Html.EditorFor(model => model.Name, new { htmlAttributes = new { @class = "form-control", placeholder = "Car Name", required = "required", @readonly = "readonly" } })
                                            @*@Html.ValidationMessageFor(model => model.Name, "", new { @class = "text-danger" })*@
                                            <div class="input-group-append">
                                                <span class="input-group-text"><i class="fa fa-tag"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>عنوان المحرك</label>
                                        <div class="input-group mb-2">
                                            @Html.EditorFor(model => model.NameAr, new { htmlAttributes = new { @class = "form-control", placeholder = "اسم المنتج", dir = "rtl", @readonly = "readonly" } })

                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fa fa-tag"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Slug</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" required readonly name="Slug" id="Slug" placeholder="Enter car name" value="@Model.Slug" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-link"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">VIN / Chasis Number</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" readonly name="SKU" id="SKU" placeholder="Enter sku name" value="@Model.SKU" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-link"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Body Condition</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" required readonly name="Condition" id="Condition" value="@Model.Condition" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-tag"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Mechanical condition</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" required readonly name="MechanicalCondition" id="MechanicalCondition" value="@Model.MechanicalCondition" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-tag"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Ads Reference Code</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" required readonly name="MechanicalCondition" id="MechanicalCondition" value="@Model.AdsReferenceCode" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-tag"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Fuel Type</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" readonly name="FuelType" id="FuelType" value="@Model.FuelType" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-tag"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Motor Make</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" required readonly name="CarMake" id="CarMake" value="@Model.CarMake.Name" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-tools"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Motor Model</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" readonly name="CarModel" id="CarModel" value="@Model.CarModel.Name" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-car"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Category</label>
                                        <div class="input-group mb-3">
                                            <input type="text" class="form-control" readonly name="SKU" id="CategoryID" placeholder="Enter Motor name" value="@(Model.Category != null ?Model.Category.CategoryName: "-" )" />

                                            <div class="input-group-append">
                                                <span class="input-group-text"><i class="fa fa-car"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <row>
                                            <label class="control-label">Body Type</label>

                                        </row>

                                        <div class="input-group mb-3">
                                            <input type="text" class="form-control" required readonly name="CarMakeID" id="CarMakeID" placeholder="Enter Motor name" value="@(Model.BodyType != null ?Model.BodyType.Name: "-" )" />

                                            <div class="input-group-append">
                                                <span class="input-group-text"><i class="fa fa-car"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Year</label>

                                        <div class=" row"></div>
                                        <div class="input-group mb-3">
                                            <input type="text" class="form-control" required readonly name="Year" id="Year" placeholder="" value="@(Model.Year != null ?Model.Year : "-" )" />

                                            <div class="input-group-append">
                                                <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Doors</label>

                                        <div class="input-group mb-3">

                                            <input type="text" class="form-control" required readonly name="Doors" id="Doors" placeholder="" value="@(Model.Doors != null ?Model.Doors : "-" )" />

                                            <div class="input-group-append">
                                                <span class="input-group-text"><i class="fa fa-car"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
