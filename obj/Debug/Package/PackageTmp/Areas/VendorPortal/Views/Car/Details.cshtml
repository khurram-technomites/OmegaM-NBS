                                        <div class="input-group mb-2">
                                            <textarea name="LongDescriptionAr" id="LongDescriptionAr" form="btnFormSubmit" required class="form-control" rows="3">@Model.LongDescriptionAr</textarea>
                                        </div>
                                    </div>
