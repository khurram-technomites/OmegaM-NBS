                                            <input type="text" class="form-control" required readonly name="Doors" id="Doors" placeholder="" value="@Model.Wheels" />

                                            <div class="input-group-append">
                                                <span class="input-group-text"><i class="fa fa-car"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Steering Side</label>

                                        <div class="input-group mb-3">

                                            <input type="text" class="form-control" required readonly name="Doors" id="Doors" placeholder="" value="@Model.SteeringSide" />

                                            <div class="input-group-append">
                                                <span class="input-group-text"><i class="fa fa-car"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Cylinders</label>
                                        <div class="input-group mb-3">
                                            <input type="text" class="form-control" readonly required name="Cylinders" id="Cylinders" placeholder="" value="@Model.Cylinders" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-tools"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <row>
                                            <label class="control-label">Horse Power</label>
                                        </row>

                                        <div class="input-group mb-3">
                                            @Html.EditorFor(model => model.HorsePower, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                            @*@Html.ValidationMessageFor(model => model.HorsePower, "", new { @class = "text-danger" })*@
                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-car"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <row>
                                            <label class="control-label">Regional Specification</label>
                                        </row>

                                        <div class="input-group mb-3">
                                            <input type="text" class="form-control" readonly required name="RegionalSpecification" id="RegionalSpecification" placeholder="" value="@Model.RegionalSpecification" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-id-card"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="row">
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <row>
                                            <label class="control-label">Service History</label>
                                        </row>

                                        <div class="input-group mb-3">
                                            <input type="text" class="form-control" readonly required name="ServiceHistory" id="ServiceHistory" placeholder="" value="@Model.ServiceHistory" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-id-card"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Capacity</label>
                                        <div class="input-group mb-2">
                                            @Html.EditorFor(model => model.Capacity, new { htmlAttributes = new { @class = "form-control", placeholder = "Car Name", required = "required", @readonly = "readonly" } })
                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-users"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>

                            <div class="row">
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Transmission</label>
                                        <div class="input-group mb-2">
                                            <input type="number" class="form-control" readonly required name="Transmission" id="Transmission" placeholder="Enter year" value="@Model.Transmission" />
                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-link"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Kilometers</label>
                                        <div class="input-group mb-2">
                                            <input type="number" class="form-control" readonly required name="FuelEconomy" id="FuelEconomy" placeholder="Enter year" value="@Model.FuelEconomy" />
                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-gas-pump"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
