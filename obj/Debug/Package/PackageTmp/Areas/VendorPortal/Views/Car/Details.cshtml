                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-money-bill"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Warranty</label>
                                        <div class="input-group mb-2">
                                            <input type="number" class="form-control" readonly required name="Warranty" id="Warranty" placeholder="" value="@Model.Warranty" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-money-bill"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*<div class="col-md-6 col-sm-12">
                                        <div class="form-group">
                                            <label class="control-label">Zip Code</label>
                                            <div class="input-group mb-2">
                                                <input type="number" class="form-control" readonly required name="Zip Code" id="Zip Code"  value="@Model.ZipCode" />

                                                <div class="input-group-append">
                                                    <span class="input-group-text">
                                                        <i class="fa fa-money-bill"></i>
