                                            <div class="input-group-append"><span class="input-group-text"><i class="fa fa-hashtag"></i></span></div>
                                        </div>
                                        @Html.ValidationMessageFor(model => model.Longitude, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Description</label>
                                        <div class="input-group mb-3">
                                            <textarea name="LongDescription" id="LongDescription" form="btnFormSubmit" required class="form-control" rows="3">@Model.LongDescription</textarea>

                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12 col-sm-12">
