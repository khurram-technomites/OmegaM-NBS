                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">City</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" readonly name="SKU" id="SKU" value="@Model.City.Name" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-map"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Car Location</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" required readonly name="Address" id="Address" value="@Model.Address" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-globe"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Area</label>
                                        <div class="input-group mb-2">
                                            <input type="text" class="form-control" readonly name="SKU" id="SKU" value="@Model.Area" />

                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="fa fa-globe"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-12">
                                    <div class="form-group">
                                        <label class="control-label">Latitude</label>
