                                            @*@Html.EditorFor(model => model.Latitude, new { htmlAttributes = new { @class = "form-control", placeholder = "Enter latitude here ...", required = "required" } })*@
                                            <input class="form-control" name="Latitude" id="Latitude" type="text" value="@Model.Latitude" readonly />
                                            <div class="input-group-append"><span class="input-group-text"><i class="fa fa-hashtag"></i></span></div>
                                        </div>
                                        @Html.ValidationMessageFor(model => model.Latitude, "", new { @class = "text-danger" })
