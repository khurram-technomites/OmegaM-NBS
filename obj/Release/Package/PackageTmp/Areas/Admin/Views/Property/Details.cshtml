@model NowBuySell.Data.Property

@{
    ViewBag.Title = "Property Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css"/>
<link href="~/Assets/Admin/css/vendor/property/edit.css" rel="stylesheet" />

<style>
    body.modal-open {
        height: 100vh;
        overflow: visible !important;
        padding-right: 15px;
    }
    fieldset {
        min-width: 0;
        padding: 0;
        margin: 0;
        border: 0;
    }
</style>

@Html.AntiForgeryToken()
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-12 subheader-transparent" id="kt_subheader">
        <div class="container-fluid align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Heading-->
                <div class="d-flex flex-column">

                    <!--begin::Breadcrumb-->
                    <div class="d-flex align-items-center font-weight-bold my-2">
                        <!--begin::Item-->
                        <a href="/Vendor/Dashboard/Index" class="opacity-75 hover-opacity-100">
                            <i class="flaticon2-shelter text-white icon-1x"></i>
                        </a>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <a href="/Vendor/Dashboard/Index" class="text-white text-hover-white opacity-75 hover-opacity-100">&nbsp;&nbsp;Dashboard</a>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <span class="label label-dot label-sm bg-white opacity-75 mx-3"></span>
                        <a href="javascript: window.history.go(-1);" class="text-white text-hover-white opacity-75 hover-opacity-100">Property</a>
                        <span class="label label-dot label-sm bg-white opacity-75 mx-3"></span>
                        <a href="javascript:;" class="text-white text-hover-white opacity-75 hover-opacity-100">Edit</a>
                        <!--end::Item-->
                    </div>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Heading-->
            </div>
            <!--end::Info-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <!--begin::Dashboard-->
            <div class="row">
                <div class="col-md-12">
                    <div class="card card-custom  gutter-b">
                        <div class="card-body">
                            <!--begin::Accordion-->
                            <div class="accordion accordion-light accordion-toggle-plus accordion-light-borderless accordion-svg-toggle" id="accordionBasicDetails">
                                <div class="card">
                                    <div class="card-header" id="headingBasicDetails">
                                        <div class="card-title" data-toggle="collapse" data-target="#collapseBasicDetails">
                                            <div class="card-label pl-4">About Property  </div>
                                        </div>
                                    </div>
                                    <div id="collapseBasicDetails" class="collapse show" data-parent="#accordionBasicDetails">
                                        <div class="card-body pl-12">
                                            <form id="btnFormSubmit">
                                                <fieldset disabled>
                                                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                                    @Html.HiddenFor(model => model.ID)

                                                    <div class="row">
                                                        <div class="col-md-6 col-sm-12">
                                                            <div class="form-group">
                                                                <label class="control-label">Ads Reference Code</label>
                                                                <div class="input-group mb-2">
                                                                    <div class="input-group-prepend" readonly>
                                                                        <span class="input-group-text">
                                                                            <i class="fa fa-hashtag"></i>
                                                                        </span>
                                                                    </div>
                                                                    <input type="text" class="form-control" required readonly name="AdsReferenceCode" id="AdsReferenceCode" value="@Model.AdsReferenceCode" />
                                                                    @Html.ValidationMessageFor(model => model.AdsReferenceCode, "", new { @class = "text-danger" })

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 col-sm-12">
                                                            <div class="form-group">
                                                                <label class="control-label">Slug</label>
                                                                <div class="input-group mb-2">
                                                                    <div class="input-group-prepend" readonly>
                                                                        <span class="input-group-text">
                                                                            <i class="fa fa-link"></i>
                                                                        </span>
                                                                    </div>
                                                                    <input type="text" class="form-control" required readonly name="Slug" id="Slug" placeholder="Slug" value="@Model.Slug" />
                                                                    @Html.ValidationMessageFor(model => model.Slug, "", new { @class = "text-danger" })

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        @{
                                                            var domainsList = new SelectList(new[]
                                                            {
                                                            new SelectListItem { Text = "Select Type", Value = ""},
                                                            new SelectListItem { Text = "Rent", Value = false.ToString()},
                                              new SelectListItem { Text = "Sale", Value = true.ToString()},
                                        }, "Value", "Text");
                                                        }

                                                        <div class="col-md-6 col-sm-12">
                                                            <div class="form-group">
                                                                <label class="control-label">Type</label>
                                                                <div class="input-group mb-2">
                                                                    @Html.DropDownListFor(model => model.ForSale, domainsList, htmlAttributes: new { @class = "form-control", required = "required" })
                                                                    <div class="input-group-append">
                                                                        <span class="input-group-text"><i class="fas fa-truck-loading"></i></span>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 col-sm-12">
                                                            <div class="form-group">
                                                                <label class="control-label">Property Category</label>
                                                                <div class="input-group mb-2">
                                                                    @Html.DropDownList("CategoryId", null, "Select Category", htmlAttributes: new { @class = "form-control", required = "required" })
                                                                    @Html.ValidationMessageFor(model => model.CategoryId, "", new { @class = "text-danger" })
                                                                    <div class="input-group-append">
                                                                        <span class="input-group-text"><i class="far fa-building"></i></span>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Title</label>
                                                                <div class="input-group mb-2">
                                                                    @Html.EditorFor(model => model.Title, new { htmlAttributes = new { @class = "form-control", required = "required" } })
                                                                    @Html.ValidationMessageFor(model => model.Title, "", new { @class = "text-danger" })
                                                                    <div class="input-group-append">
                                                                        <span class="input-group-text"><i class="fa fa-tag"></i></span>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Title (Ar)</label>
                                                                <div class="input-group mb-2">
                                                                    <div class="input-group-prepend">
                                                                        <span class="input-group-text"><i class="fa fa-tag"></i></span>
                                                                    </div>
                                                                    @Html.EditorFor(model => model.TitleAr, new { htmlAttributes = new { @class = "form-control", required = "required", dir = "rtl" } })
                                                                    @Html.ValidationMessageFor(model => model.TitleAr, "", new { @class = "text-danger" })

                                                                </div>
                                                            </div>
                                                        </div>






                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-8">
                                                            <div class="row">
                                                                <div class="col-md-12 col-sm-12">

                                                                    <div class="form-group tab-option">
                                                                        <label class="control-label">Number of Rooms</label>
                                                                        <div class="input-group mb-3">
                                                                            <div class="input-group-prepend" readonly>
                                                                                <span class="input-group-text"><i class="fa fa-bed"></i></span>
                                                                            </div>
                                                                            <div class="tab-options">
                                                                                <span>0</span>
                                                                                <span>1</span>
                                                                                <span>2</span>
                                                                                <span>3</span>
                                                                                <span>4</span>
                                                                                <span>5</span>
                                                                                <span>6</span>
                                                                                <span>7</span>
                                                                                <span>8</span>
                                                                                <span>9</span>
                                                                                <span>10</span>
                                                                                <span>11</span>
                                                                                <span>12+</span>
                                                                            </div>
                                                                            @Html.EditorFor(model => model.NoOfRooms, new { htmlAttributes = new { required = "required", oninput = "value == '' ? value = '' : value < 0 ? value = value * -1 : false", type = "number", min = "0" } })
                                                                            @Html.ValidationMessageFor(model => model.NoOfDinings, "", new { @class = "text-danger" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-6 col-sm-12">
                                                                    <div class="form-group">
                                                                        <label class="control-label">Number of Baths</label>
                                                                        <div class="input-group mb-3">
                                                                            <div class="input-group-prepend" readonly>
                                                                                <span class="input-group-text"><i class="fa fa-bath"></i></span>
                                                                            </div>
                                                                            <div class="tab-options">
                                                                                <span>0</span>
                                                                                <span>1</span>
                                                                                <span>2</span>
                                                                                <span>3</span>
                                                                                <span>4</span>
                                                                                <span>5+</span>
                                                                            </div>
                                                                            @Html.EditorFor(model => model.NoOfBaths, new { htmlAttributes = new { required = "required", oninput = "value == '' ? value = '' : value < 0 ? value = value * -1 : false", type = "number", min = "0" } })
                                                                            @Html.ValidationMessageFor(model => model.Size, "", new { @class = "text-danger" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-6 col-sm-12">
                                                                    <div class="form-group">
                                                                        <label class="control-label">Number of Dinning</label>
                                                                        <div class="input-group mb-3">
                                                                            <div class="input-group-prepend" readonly>
                                                                                <span class="input-group-text"><i class="fas fa-utensils"></i></span>
                                                                            </div>
                                                                            <div class="tab-options">
                                                                                <span>1</span>
                                                                                <span>2</span>
                                                                                <span>3</span>
                                                                            </div>
                                                                            @Html.EditorFor(model => model.NoOfDinings, new { htmlAttributes = new { type = "number", oninput = "value == '' ? value = '' : value < 0 ? value = value * -1 : false", min = "0" } })
                                                                            @Html.ValidationMessageFor(model => model.NoOfDinings, "", new { @class = "text-danger" })

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-6 col-sm-12">
                                                                    <div class="form-group">
                                                                        <label class="control-label">Number of Laundry</label>
                                                                        <div class="input-group mb-3">
                                                                            <div class="input-group-prepend" readonly>
                                                                                <span class="input-group-text"><i class="fa fa-tshirt"></i></span>
                                                                            </div>
                                                                            <div class="tab-options">
                                                                                <span>1</span>
                                                                                <span>2</span>
                                                                                <span>3</span>
                                                                            </div>
                                                                            @Html.EditorFor(model => model.NoOfLaundry, new { htmlAttributes = new { type = "number", oninput = "value == '' ? value = '' : value < 0 ? value = value * -1 : false", min = "0" } })
                                                                            @Html.ValidationMessageFor(model => model.NoOfDinings, "", new { @class = "text-danger" })

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-4 col-sm-12">
                                                                    <div class="form-group">
                                                                        <label class="control-label">Number of Garage</label>
                                                                        <div class="input-group mb-3">
                                                                            <div class="input-group-prepend" readonly>
                                                                                <span class="input-group-text"><i class="fa fa-warehouse"></i></span>
                                                                            </div>
                                                                            <div class="tab-options">
                                                                                <span>1</span>
                                                                                <span>2</span>
                                                                                <span>3</span>
                                                                            </div>
                                                                            @Html.EditorFor(model => model.NoOfGarage, new { htmlAttributes = new { type = "number", oninput = "value == '' ? value = '' : value < 0 ? value = value * -1 : false", min = "0" } })
                                                                            @Html.ValidationMessageFor(model => model.NoOfDinings, "", new { @class = "text-danger" })

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-2 col-sm-12">
                                                                    <div class="form-group">
                                                                        <label class="control-label">Furnished</label>
                                                                        <span class="switch">
                                                                            <label>
                                                                                @Html.CheckBox("IsFurnished")
                                                                                <span></span>
                                                                            </label>
                                                                        </span>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <div class="row">
                                                                <div class="col-md-12 col-sm-12">
                                                                    <div class="form-group">
                                                                        <label class="control-label">Build Year</label>
                                                                        <div class="input-group mb-2">
                                                                            @*<input class="form-control carYear" id="BuildYear" required/>*@
                                                                            @Html.EditorFor(model => model.BuildYear, new { htmlAttributes = new { @class = "form-control", type = "text" } })
                                                                            @Html.ValidationMessageFor(model => model.BuildYear, "", new { @class = "text-danger" })
                                                                            <div class="input-group-append">
                                                                                <span class="input-group-text">
                                                                                    <i class="fa fa-calendar"></i>

                                                                                </span>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-12 col-sm-12">
                                                                    <div class="form-group">
                                                                        <label class="control-label">Rent (Yearly)</label>
                                                                        <div class="input-group mb-2">
                                                                            @Html.EditorFor(model => model.Price, new { htmlAttributes = new { @class = "form-control", required = "required", oninput = "value == '' ? value = '' : value < 0 ? value = value * -1 : false", type = "number", min = "0" } })
                                                                            @Html.ValidationMessageFor(model => model.Price, "", new { @class = "text-danger" })
                                                                            <div class="input-group-append">
                                                                                <span class="input-group-text">
                                                                                    <i class="fas fa-money-bill"></i>
                                                                                </span>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-12 col-sm-12">
                                                                    <div class="form-group">
                                                                        <label class="control-label">Size (sq ft)</label>
                                                                        <div class="input-group mb-3">
                                                                            @Html.EditorFor(model => model.Size, new { htmlAttributes = new { @class = "form-control", required = "required", oninput = "value == '' ? value = '' : value < 0 ? value = value * -1 : false", type = "number", min = "0" } })
                                                                            @Html.ValidationMessageFor(model => model.Size, "", new { @class = "text-danger" })
                                                                            <div class="input-group-append">
                                                                                <span class="input-group-text"><i class="fa fa-crop"></i></span>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                @*<div class="col-md-12 col-sm-12">
                                                                        <div class="form-group">
                                                                            <label class="control-label">Furnished</label>
                                                                            <div class="input-group mb-2">
                                                                                <select id="IsFurnished" name="IsFurnished" required class="form-control">
                                                                                    <option value="">Select Property Is Furnished...</option>
                                                                                    <option value="1">Yes</option>
                                                                                    <option value="0">No</option>
                                                                                </select>
                                                                                <div class="input-group-append">
                                                                                    <span class="input-group-text">
                                                                                        <i class="fa fa-id-card"></i>
                                                                                    </span>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>*@
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-6 col-sm-12">
                                                            <div class="form-group">
                                                                <label class="control-label">Description</label>
                                                                <div class="mb-2">
                                                                    <textarea name="Description" id="Description" form="btnFormSubmit" class="form-control" placeholder="Enter Description here .." rows="3">@Model.Description</textarea>
                                                                    @Html.ValidationMessageFor(model => model.Description, "", new { @class = "text-danger" })
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 col-sm-12">
                                                            <div class="form-group">
                                                                <label class="control-label">Description (Ar)</label>
                                                                <div class="mb-2">
                                                                    <textarea name="DescriptionAr" id="DescriptionAr" form="btnFormSubmit" style="text-align:right" class="form-control" placeholder="Enter Description here .." rows="3">@Model.DescriptionAr</textarea>
                                                                    @Html.ValidationMessageFor(model => model.DescriptionAr, "", new { @class = "text-danger" })
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </fieldset>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 col-sm-12">
                    <div class="card card-custom gutter-b card-stretch">
                        <div class="card-body p-10">
                            <!--begin::Accordion-->
                            <div class="accordion accordion-light accordion-toggle-plus accordion-light-borderless accordion-svg-toggle" id="accordionGallery">
                                <div class="card">
                                    <div class="card-header" id="headingGallery">
                                        <div class="card-title" data-toggle="collapse" data-target="#collapseGallery">
                                            <div class="card-label pl-4">Property Gallery</div>
                                        </div>
                                    </div>
                                    <div id="collapseGallery" class="collapse show" data-parent="#accordionGallery">
                                        <div class="card-body pl-12 text-center">
                                            <!--begin::Car images-->
                                            <div class="Property-gallery-images mb-5">
                                                <div class="spinner spinner-dark spinner-center"></div>
                                            </div>
                                            <input type="file" name="GalleryImages" value="" multiple style="display:none" accept=".png, .jpg, .jpeg" />
                                            <!--end::Car images-->
                                            <!--<a href="javascript:;" class="btn btn-light-primary " id="btn-gallery-image-upload">
                                            <span class="svg-icon svg-icon-lg m-0">-->
                                            <!--begin::Svg Icon | path:assets/media/svg/icons/Design/Image.svg-->
                                            <!--<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
                                                <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                                                    <polygon points="0 0 24 0 24 24 0 24"></polygon>
                                                    <path d="M6,5 L18,5 C19.6568542,5 21,6.34314575 21,8 L21,17 C21,18.6568542 19.6568542,20 18,20 L6,20 C4.34314575,20 3,18.6568542 3,17 L3,8 C3,6.34314575 4.34314575,5 6,5 Z M5,17 L14,17 L9.5,11 L5,17 Z M16,14 C17.6568542,14 19,12.6568542 19,11 C19,9.34314575 17.6568542,8 16,8 C14.3431458,8 13,9.34314575 13,11 C13,12.6568542 14.3431458,14 16,14 Z" fill="#000000"></path>
                                                </g>
                                            </svg>-->
                                            <!--end::Svg Icon-->
                                            <!--</span>Upload
                                            </a>-->
                                            <span class="form-text text-muted" style="font-size: 0.9rem; font-weight: 400;">Allowed file types: png, jpg, jpeg.</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--end::Accordion-->
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12 col-sm-12">
                    <div class="card card-custom gutter-b">
                        <div class="card-body p-10">
                            <!--begin::Accordion-->
                            <div class="accordion accordion-light accordion-toggle-plus accordion-light-borderless accordion-svg-toggle" id="accordionFeatures">
                                <div class="card">
                                    <div class="card-header" id="headingFeatures">
                                        <div class="card-title" data-toggle="collapse" data-target="#collapseFeatures">
                                            <div class="card-label pl-4">Location</div>
                                        </div>
                                    </div>
                                    <div id="collapseFeatures" class="collapse show" data-parent="#accordionFeatures">
                                        <div class="card-body pl-12">
                                            <form id="PropertyLocationForm">
                                                <fieldset disabled>
                                                    <div class="row">

                                                        <div class="col-md-6 col-sm-12">
                                                            <div class="form-group">
                                                                <label class="control-label">City</label>
                                                                <div class="input-group mb-3">
                                                                    @*@Html.HiddenFor(x => x.PropertyAddresses[0].CityId)*@
                                                                    @Html.DropDownList("CityID", null, "Select City", htmlAttributes: new { @class = "form-control", required = "required" })
                                                                    @Html.ValidationMessageFor(model => model.CityID, "", new { @class = "text-danger" })

                                                                    <div class="input-group-append">
                                                                        <span class="input-group-text"><i class="fa fa-map"></i></span>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 col-sm-12">
                                                            <div class="form-group">
                                                                <label class="control-label">Area</label>
                                                                <div class="input-group mb-3">
                                                                    <input type="text" class="form-control" min="0" required name="Area" id="Area" placeholder="Enter area" value="@Model.Area" />
                                                                    <div class="input-group-append">
                                                                        <span class="input-group-text"><i class="fa fa-road"></i></span>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-12 col-sm-12">
                                                            <div class="form-group w-100">
                                                                <label class="wo-titleinput mb-0">Search Location</label>
                                                                <div class="wo-form-icon">
                                                                    <input type="text" class="form-control show-map-input Address" id="Address" name="Address" placeholder="Choose location" required value="@Model.Address">
                                                                </div>
                                                            </div>
                                                            <div class="MapSearchResult" style="display:none">
                                                            </div>
                                                            <input type="hidden" name="Latitude" id="Latitude" value="@Model.Latitude" />
                                                            <input type="hidden" name="Longitude" id="Longitude" value="@Model.Longitude" />
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12">
                    <div class="card card-custom gutter-b">
                        <div class="card-body p-10">
                            <!--begin::Accordion-->
                            <div class="accordion accordion-light accordion-toggle-plus accordion-light-borderless accordion-svg-toggle" id="accordionFeatures">
                                <div class="card">
                                    @*<div class="card-header" id="headingFeatures">
                                            <div class="card-title" data-toggle="collapse" data-target="#collapseFeatures">
                                                <div class="card-label pl-4">Features</div>
                                            </div>
                                            <a class="pl-4" href="javascript:;" onclick="OpenModelPopup(this,'/Vendor/Property/addfeature')">Request New</a>
                                        </div>*@
                                    <div class="card-header" id="headingFeatures">
                                        <div class="card-title" data-toggle="collapse" data-target="#collapseFeatures">
                                            <div class="card-label pl-4">Features</div>
