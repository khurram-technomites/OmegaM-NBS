@section IndexScript {
    <script src="~/assets/admin/plugins/custom/ckeditor/ckeditor-classic.bundle.js"></script>

    <script src="~/assets/admin/js/pages/crud/forms/widgets/bootstrap-datepicker.js"></script>
    <!--end::Page Vendors-->
    <!--begin::Page Scripts(used by this page)-->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAEmhLaFjth5xau57Gy1NwE1O6apk443xY&callback=initMap&libraries=places&callback=initAutocomplete"></script>

    <script src="~/Assets/Admin/js/general/ModalPopup.js"></script>
    <script src="~/Assets/Admin/js/general/Map.js"></script>


    <script src="@Url.ContentVersioned("~/Assets/Admin/js/admin/Property/details.js")"></script>

    <script>
        var slideIndex = 1;
        showDivs(slideIndex);

        function plusDivs(n) {
            slideIndex = parseInt($("#imagepreview").attr("data-id"));
            showDivs(slideIndex += n);
        }

        function showDivs(n) {
            var i;
            var x = document.getElementsByClassName("property-image");

            if (n >= x.length) { slideIndex = 0 }
            if (n < 0) { slideIndex = x.length - 1 }
            for (i = 0; i < x.length; i++) {
                $('#imagepreview').attr('src', x[slideIndex].style.backgroundImage.slice(5, -2))
                $('#imagepreview').attr('data-id', x[slideIndex].dataset.id)
            }
            /* x[slideIndex - 1].style.display = "block";*/
        }
        var Description, DescriptionAr;

        $(document).ready(function () {
            $("#NRValidation").hide()
            $("#NBValidation").hide()

            ClassicEditor
               .create(document.querySelector('#Description'))
               .then(editor => {
                   Description = editor;
                   Description.setData(`@Html.Raw(Model.Description)`);
                   editor.isReadOnly = true; // make the editor read-only right after initialization
               })
               .catch(error => {
                   console.log(error);
               });

            ClassicEditor
                .create(document.querySelector('#DescriptionAr'))
                .then(editor => {
                    DescriptionAr = editor;
                    DescriptionAr.setData(`@Html.Raw(Model.DescriptionAr)`);
                    editor.isReadOnly = true; // make the editor read-only right after initialization
                })
                .catch(error => {
                    console.log(error);
                });

            if ('@Model.CategoryId' != null) {
                $("#CategoryId").val('@Model.CategoryId')
            }

            if ('@Model.ForSale' != null) {
                $("#ForSale").val('@Model.ForSale.ToString()')
            }

            $('input[name=NoOfRooms]').closest('.form-group').find(`span:contains(${@(Model.NoOfRooms.HasValue?Model.NoOfRooms.Value:-1)})`).addClass('active');
            $('input[name=NoOfBaths]').closest('.form-group').find(`span:contains(${@(Model.NoOfBaths.HasValue?Model.NoOfBaths.Value:-1)})`).addClass('active');
            $('input[name=NoOfDinings]').closest('.form-group').find(`span:contains(${@(Model.NoOfDinings.HasValue?Model.NoOfDinings.Value:0)})`).addClass('active');
            $('input[name=NoOfLaundry]').closest('.form-group').find(`span:contains(${@(Model.NoOfLaundry.HasValue?Model.NoOfLaundry.Value:0)})`).addClass('active');
            $('input[name=NoOfGarage]').closest('.form-group').find(`span:contains(${@(Model.NoOfGarage.HasValue?Model.NoOfGarage.Value:0)})`).addClass('active');
        })

    </script>

}
