	<link rel="stylesheet" href="~/Assets/css/main-custom.css">
	<link rel="stylesheet" href="@Url.ContentVersioned("~/Assets/css/layout.css")">
	<link rel="stylesheet" href="@Url.ContentVersioned("~/Assets/css/web-custom-style"+ ( RTL ? "-rtl" : "") + ".css")">
	<link rel="stylesheet" href="@Url.ContentVersioned("~/Assets/css/custom-responsive.css")">
</head>

<body class="wo-homepage">
	<!-- Preloader Start -->
	<div class="preloader-outer">
		<div class="loader"></div>
	</div>
	<!-- Preloader End -->
	<!-- MAIN BODY START -->
	<main class="wo-main overflow-hidden" style="padding: 50px 30px">
		<!-- Appreciation Start -->
		<section class="wo-main-section">
			<div class="container">
				<div class="row">
					<div class="col-12">
						<div class="wo-404error">
							<figure class="wo-404error__img">
								<img src="~/Assets/images/500.png" alt="img description">
							</figure>
							<div class="wo-errorcontent">
								<h3>Whoops...</h3>
								<p>Something went wrong. Maybe our FAQ or Community can help?</p>
                                <a href="javascript:history.back()" class="wo-btn">Go Back</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
		<!-- Appreciation End -->
	</main>
	<!-- MAIN BODY END -->

	<script src="~/Assets/js/vendor/jquery.min.js"></script>
	<script src="~/Assets/js/vendor/jquery-library.js"></script>
